<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lolo.Mybatis.mapper.SQLMapper">

<select id="testMohu" resultType="User">
    select * from table_employee where last_name like "%" #{mohu} "%"

</select>
    <delete id="deleteMore">
        delete  from table_employee where  id in (${ids})
    </delete>
<!--    List<User>getUserByTableName(@Param("tableName") String tableName);-->
<select id="getUserByTableName" resultType="User">
    select * from ${tableName}
</select>
    <insert id="insertUser" useGeneratedKeys="true" keyProperty="id">
        insert into  table_employee values(null,#{last_name},#{email}, #{salary})
    </insert>
</mapper>
